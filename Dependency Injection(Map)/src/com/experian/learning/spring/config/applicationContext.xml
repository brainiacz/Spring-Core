<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
	   					   http://www.springframework.org/schema/beans/spring-beans.xsd">
	   	
	<bean id="college" class="com.experian.learning.spring.pojo.College">
		<property name="facultySubjects">
			<map>
				<entry key="Raja" value="java"/>
				<entry key="Rahul" value=".Net"/>
				<entry key="Aishwarya" value="CS"></entry>
			</map>
		</property>
		
		<property name="facultyPhones">
			<map>
				<entry key="Raja" value="012"></entry>
				<entry key="Rahul" value="013"></entry>
				<entry key="Aishwarya" value="015"></entry>
			</map>
		</property>
	</bean>
</beans>